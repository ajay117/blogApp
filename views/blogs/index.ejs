<%- include("../partials/head") %>

<h1 class="text-center">All Blogs</h1>

<p class="text-center"><a href="/">Go to main page.</a></p>

<% for(let blog of blogData) { %>
<div class="container">
  <div class="img-container">
    <img src="<%= blog.image %>" />
  </div>
  <p><%= blog.title %></p>
  <p><%= blog.description %></p>
  <p>
    <a href="/blogs/<%= blog.id %>">See More</a>
  </p>
</div>

<% } %> <%- include("../partials/footer") %>
